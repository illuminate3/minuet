{% extends 'layout/dash/dash.html.twig' %}

{% block subheader %}
<!-- Title -->
{{ include('layout/partials/_title.html.twig') }}
{% endblock %}

{% block body %}

<div class="row mt-4 mb-4">
    <div class="col">

        {% form_theme form 'bootstrap_5_layout.html.twig' %}

        {{ form_start(form) }}

        {{ form_row(form.full_name) }}
        {{ form_row(form.phone) }}

        <!-- Submit -->
        {{ include('layout/dash/partials/_submit.html.twig') }}

        {{ form_end(form) }}

    </div>
</div>

{% endblock %}
