{% extends 'layout/dash/dash.html.twig' %}

{% block subheader %}
    <!-- Title -->
    {{ include('layout/partials/_title.html.twig') }}
{% endblock %}

{% block body %}

    <div class="row mt-4 mb-4">
        <div class="col">

            <h1>
                {{ title|trans }}
            </h1>

        </div>
    </div>

    {% if is_primary == true %}

        {% for data in account_users %}
            <p>
                {#        {{ dump(data) }}#}
                {#        {{ dump(data.user.profile.firstName) }}#}
                {{ data.user.email }}
                --
                {{ data.user.profile.firstName }}
            </p>
        {% endfor %}

        <p>
            {#        {{ dump(subscription) }}#}
            {{ subscription.plan }}
        </p>

    {% endif %}



    <p>
{#        {{ dump(account) }}#}
        {{ account.name }}
    </p>


    {% for data in products %}
    <p>
{#        {{ dump(data) }}#}
{#        {{ dump(data.product.id) }}#}
        {{ data.id }}
        --
        {{ data.title }}
    </p>
    {% endfor %}


{% endblock %}
