{% extends 'layout/base.html.twig' %}

{% block title %}{{ 'title.dash' | trans }}{% endblock %}

{% block body %}

    <br>

    {% for flash_error in app.flashes('verify_email_error') %}
        <div class="alert alert-danger" role="alert">{{ flash_error }}</div>
    {% endfor %}

    <div class="row justify-content-center mb-5">
        <div class="col-md-6">
            <div class="card">
                <div class="card-header">{{ 'title.register' | trans }}</div>
                <div class="card-body">


                    <ul class="breadcrumb">
                        <li class="breadcrumb-item"><a href="">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="{{ path('user_profile') }}">Profile</a></li>
                        <li class="breadcrumb-item"><a href="{{ path('security_login') }}">login</a></li>
                        <li class="breadcrumb-item"><a href="{{ path('security_logout') }}">logout</a></li>
                        <li class="breadcrumb-item"><a href="{{ path('register') }}">Register</a></li>
                    </ul>



                </div>
            </div>
        </div>
    </div>

{% endblock %}
