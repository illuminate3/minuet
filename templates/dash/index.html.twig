{% extends 'layout/dash/dash.html.twig' %}

{% block subheader %}
    <!-- Title -->
    {{ include('layout/partials/_title.html.twig') }}
{% endblock %}

{% block body %}

    <div class="row mt-4 mb-4">
        <div class="col">

            <h1>
                {{ title|trans }}
            </h1>

        </div>
    </div>

    <p>
{#        {{ dump(subscription) }}#}
        {{ subscription.plan }}
    </p>

    <p>
{#        {{ dump(account) }}#}
        {{ account.name }}
    </p>

    {% for data in usersData %}
    <p>
{#        {{ dump(data) }}#}
{#        {{ dump(data.user.profile.firstName) }}#}
        {{ data.user.email }}
        --
        {{ data.user.profile.firstName }}
    </p>
    {% endfor %}

    {% for data in listingData %}
    <p>
{#        {{ dump(data) }}#}
{#        {{ dump(data.product.id) }}#}
        {{ data.product.id }}
        --
        {{ data.product.title }}
    </p>
    {% endfor %}


{% endblock %}
